<%- include('header') -%>

<nav class="navbar navbar-light bg-light">
<div id="navBar"class="container-fluid">
<span class="navbar-brand mb-0 h1">Student Lists</span>
</div>
</nav>

<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">DOB</th>
        <th scope="col">Age</th>
        <th scope="col">Mobile No</th>
        <th scope="col">E-Mail</th>
        <th scope="col">Department</th>
        <th scope="col">Address</th>
      </tr>
    </thead>
    <tbody>
      <% finalData.forEach(function(data){ %>
        <tr>
          <th scope="row"><%= data.name %></th>
          <td><%= data.dob %></td>
          <td><%= data.age %></td>
          <td><%= data.mobileNo %></td>
          <td><%= data.Email %></td>
          <td><%= data.department %></td>
          <td><%= data.address %></td>
          </tr>
      <% }) %>

      </tbody>


  </table>
  <div class="">
    <form class="" action="/delete" method="post">
      <button class="btn btn-primary btn-lg" type="submit" name="button">Edit</button>
    </form>
  </div>

</div>










<%- include("footer") -%>
